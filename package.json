{
	"name": "svelte-patterns",
	"version": "0.1.0",
	"description": "Common patterns for svelte",
	"type": "module",
	"keywords": [
		"svelte",
		"capacitor",
		"web",
		"mobile",
		"template"
	],
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"serve": "vite preview",
		"check": "svelte-check",
		"format": "prettier --write ./**/*.{js,ts,svelte,html}",
		"prettier": "prettier --write",
		"lint": "eslint --no-error-on-unmatched-pattern \"./src/**/*.{js,svelte}\"",
		"lint:fix": "eslint --no-error-on-unmatched-pattern --fix \"./src/**/*.{js,ts,svelte}\"",
		"prelint": "npm run format"
	},
	"devDependencies": {
		"@mdi/js": "^7.2.96",
		"@skeletonlabs/skeleton": "^1.12.0",
		"@sveltejs/vite-plugin-svelte": "^2.4.5",
		"@tsconfig/svelte": "^5.0.0",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"autoprefixer": "^10.4.15",
		"eslint": "^8.47.0",
		"eslint-plugin-svelte": "^2.32.4",
		"postcss": "^8.4.28",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"sass": "^1.66.1",
		"svelte": "^4.2.0",
		"svelte-check": "^3.5.0",
		"svelte-preprocess": "^5.0.4",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.2",
		"typescript": "^5.1.6",
		"vite": "^4.4.9"
	},
	"license": "MIT"
}
